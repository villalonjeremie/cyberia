PYTHON_API_KEY=SECRET123
PYTHON_API_URL=http://log_python_worker:8030
SYMFONY_TRUSTED_HOSTS=.*
APP_RUNTIME_ENV=dev
APP_RUNTIME_MODE=default

# Trusted proxies : on fait confiance à toutes les IPs
SYMFONY_TRUSTED_PROXIES=0.0.0.0/0

# Trusted headers : on fait confiance à tous les headers proxy connus
SYMFONY_TRUSTED_HEADERS=forwarded, x-forwarded-for, x-forwarded-host, x-forwarded-proto, x-forwarded-port
